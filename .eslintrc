{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
		"no-multi-spaces": 0,
		"no-use-before-define": 0,
		"no-shadow": 0,
		"no-empty": 0,
		"no-alert": 0,
		"eol-last": 0,
		"comma-dangle": 0,
		"no-path-concat": 0
    }
}
